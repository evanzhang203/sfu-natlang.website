---
title: News
layout: rss
---

{% for post in site.posts %}
<item>
	<title>{{ post.title }}</title>
	<description>
	{{ post.content }}
	</description>
	<guid>{{ post.url }}</giid>
	<link>{{ site.baseurl }}{{ post.url }}</link>
	<pubDate>{{ post.time | date_to_xmlschema }}</pubDate>
</item>
{% endfor %}
